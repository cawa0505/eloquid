{
    "name": "datakrama/eloquid",
    "description": "Eloquid - Auto UUID for Eloquent Model",
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Husen",
            "email": "hello@husen.id"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Datakrama\\Eloquid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datakrama\\Eloquid\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
