{
    "name": "datakrama/eloquid",
    "description": "Eloquid - Auto UUID for Eloquent Model",
    "type": "library",
    "require": {
        "illuminate/support": "5.8.*|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Husen",
            "email": "husenisme@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Datakrama\\Eloquid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datakrama\\Eloquid\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
